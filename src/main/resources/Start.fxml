<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="gui.project.StartController">

    <children>
        <Button prefWidth="100" layoutX="250.0" layoutY="298.0" mnemonicParsing="false" onMouseClicked="#onMouseClickedPlay"
                text="Play "/>
        <Button prefWidth="100" layoutX="250.0" layoutY="338.0" mnemonicParsing="false" onMouseClicked="#onMouseClickedRules"
                text="Rules"/>

        <Label alignment="CENTER" contentDisplay="CENTER" layoutY="200" prefHeight="20.0" prefWidth="600.0"
               stylesheets="@style.css" text="Order and Chaos" textAlignment="CENTER" textOverrun="WORD_ELLIPSIS">
            <font>
                <Font size="36"></Font>
            </font>
        </Label>
    </children>

</AnchorPane>
